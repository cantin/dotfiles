{
  "coc.preferences.extensionUpdateCheck": "never",
  "snippets.extends": {
    "javascriptreact": ["javascript", "html"],
    "typescript": ["javascript"]
  },
  "coc.source.around.priority": 5,
  "coc.source.buffer.priority": 5,
  "coc.source.tag.priority": 1,
  "typescript.validate.enable": false,
  "typescript.suggestionActions.enabled": false,
  "typescript.suggest.enabled": false,
  "javascript.suggestionActions.enabled": false,
  "yank.highlight.enable": false,
  "yank.priority": 1,
  //"solargraph.transport": "socket",
  "translator.engines": [
    "bing",
    "google",
    "youdao",
    "haici"
  ],
  "typescript.updateImportsOnFileMove.enable": false,
  "suggest.noselect": true,
  "suggest.selection": "first",
  "tailwindCSS.includeLanguages": {
    "plaintext": "html",
    "haml": "haml"
  },
  "html-css-support.enabledLanguages": [
    "html",
    "haml"
  ],
  "html.filetypes": [
    "html","handlebars","htmldjango","blade","haml"
  ]
}
